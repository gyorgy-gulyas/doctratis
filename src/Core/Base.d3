domain Core {

    context Base {

        composite BaseEntity {
            @system_field
            id:string
            @system_field
            etag:string
            @system_field
            LastUpdate:datetime
        }

        service SmsService {
            command SendOTP( phoneNumber:string, otp:string )
        }

        service EmailService {
            command SendOTP( phoneNumber:string, otp:string )
        }
    }
}
